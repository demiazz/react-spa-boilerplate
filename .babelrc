{
  "presets": ["react"],
  "plugins": ["transform-runtime"],
  "env": {
    "production": {
      "presets": ["es2015"],
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    },
    "development": {
      "presets": ["es2015", "react-hmre"]
    },
    "test": {
      "presets": ["es2015-node5"],
      "plugins": ["add-module-exports", "remove-module-hot"]
    }
  }
}
