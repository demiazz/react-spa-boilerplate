{
  "name": "react-spa-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "A boilerplate for React SPA without server rendering",
  "repository": {
    "type": "git",
    "url": "https://github.com/demiazz/react-spa-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "jss",
    "webpack",
    "express",
    "node"
  ],
  "author": "Alexey Plutalov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/demiazz/react-spa-boilerplate/issues"
  },
  "homepage": "https://github.com/demiazz/react-spa-boilerplate",
  "engines": {
    "node": ">= 5.10.0"
  },
  "scripts": {
    "start":         "NODE_ENV=development bin/server",
    "build":         "NODE_ENV=production bin/build",
    "test":          "NODE_ENV=test $(npm bin)/ava | $(npm bin)/tap-spec",
    "test:watch":    "NODE_ENV=test $(npm bin)/ava --watch | $(npm bin)/tap-spec",
    "test:coverage": "NODE_ENV=test $(npm bin)/nyc --reporter html npm test",
    "lint":          "$(npm bin)/eslint .",
    "new":           "bin/new"
  },
  "dependencies": {
    "jss":                 "3.9.1",
    "jss-vendor-prefixer": "2.0.0",
    "jss-nested":          "1.0.3",
    "jss-camel-case":      "1.1.0",
    "jss-isolate":         "0.1.0",
    "jss-default-unit":    "2.2.0",

    "react":               "15.0.2",
    "react-dom":           "15.0.2",
    "react-router":        "2.4.0",
    "redux":               "3.5.2",
    "redux-logger":        "2.6.1",
    "redux-thunk":         "2.0.1",
    "react-redux":         "4.4.5",
    "react-router-redux":  "4.0.4"
  },
  "devDependencies": {
    "babel-core":                      "6.7.7",
    "babel-register":                  "6.7.2",

    "babel-preset-es2015-node5":       "1.2.0",
    "babel-preset-stage-3":            "6.5.0",
    "babel-plugin-add-module-exports": "0.1.4",

    "babel-preset-es2015":             "6.6.0 ",
    "babel-preset-stage-0":            "6.5.0",
    "babel-preset-react":              "6.5.0",
    "babel-preset-react-hmre":         "1.1.1",
    "babel-plugin-transform-runtime":  "6.7.5",
    "babel-plugin-remove-module-hot":  "1.0.0",

    "react-addons-test-utils":         "15.0.2",
    "react-transform-hmr":             "1.0.4",

    "express":                         "4.13.4",

    "webpack":                         "1.13.0",
    "webpack-dev-middleware":          "1.6.1",
    "webpack-hot-middleware":          "2.10.0",

    "babel-loader":                    "6.2.4",
    "style-loader":                    "0.13.1",
    "css-loader":                      "0.23.1",
    "postcss-loader":                  "0.9.1",
    "json-loader":                     "0.5.4",

    "extract-text-webpack-plugin":     "1.0.1",
    "html-webpack-plugin":             "2.16.0",
    "stats-webpack-plugin":            "0.3.1",

    "autoprefixer":                    "6.3.6",

    "eslint":                          "2.9.0",
    "eslint-config-airbnb":            "8.0.0",
    "eslint-plugin-babel":             "3.2.0",
    "eslint-plugin-react":             "5.0.1",
    "eslint-plugin-jsx-a11y":          "1.0.4",
    "eslint-plugin-import":            "1.6.1",
    "babel-eslint":                    "6.0.4",

    "ava":                             "0.14.0",
    "tap-spec":                        "4.1.1",
    "jsdom":                           "8.4.0",
    "enzyme":                          "2.2.0",
    "sinon":                           "1.17.3",

    "nyc":                             "6.4.0",

    "rimraf":                          "2.5.2",
    "lodash.values":                   "4.1.0",
    "ejs":                             "2.4.1",
    "commander":                       "2.9.0",
    "uppercamelcase":                  "1.1.0",
    "mkdir-recursive":                 "0.2.1",
    "mustache":                        "2.2.1",
    "chalk":                           "1.1.3",
    "mkdirp":                          "0.5.1"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/setup.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "tap": true,
    "require": [
      "babel-register",
      "./test/setup.js"
    ],
    "babel": "inherit"
  }
}
