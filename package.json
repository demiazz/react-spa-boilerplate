{
  "name": "react-spa-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "A boilerplate for React SPA without server rendering",
  "repository": {
    "type": "git",
    "url": "https://github.com/demiazz/react-spa-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "jss",
    "webpack",
    "express",
    "node"
  ],
  "author": "Alexey Plutalov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/demiazz/react-spa-boilerplate/issues"
  },
  "homepage": "https://github.com/demiazz/react-spa-boilerplate",
  "engines": {
    "node": ">= 5.10.0"
  },
  "scripts": {
    "start":         "bin/server",
    "build":         "bin/build",
    "test":          "$(npm bin)/mocha --compilers js:babel-register --recursive",
    "test:watch":    "$(npm bin)/mocha --watch --compilers js:babel-register --recursive",
    "test:coverage": "$(npm bin)/istanbul cover --report html $(npm root)/mocha/bin/_mocha -- --compilers js:babel-register --recursive",
    "lint":          "$(npm bin)/eslint ."
  },
  "dependencies": {
    "jss":                 "3.6.3",
    "jss-vendor-prefixer": "2.0.0",
    "jss-nested":          "1.0.3",
    "jss-camel-case":      "1.0.2",

    "react":               "15.0.1",
    "react-dom":           "15.0.1",
    "react-jss":           "2.0.6",
    "react-router":        "2.2.4",
    "redux":               "3.4.0",
    "react-redux":         "4.4.5",
    "react-router-redux":  "4.0.2"
  },
  "devDependencies": {
    "babel-core":                      "6.7.6",
    "babel-register":                  "6.7.2",

    "babel-preset-es2015-node5":       "1.2.0",
    "babel-preset-stage-3":            "6.5.0",
    "babel-plugin-add-module-exports": "0.1.2",

    "babel-preset-es2015":             "6.6.0 ",
    "babel-preset-stage-0":            "6.5.0",
    "babel-preset-react":              "6.5.0",
    "babel-preset-react-hmre":         "1.1.1",
    "babel-plugin-transform-runtime":  "6.7.5",

    "react-addons-test-utils":         "15.0.1",
    "react-transform-hmr":             "1.0.4",

    "express":                         "4.13.4",

    "webpack":                         "1.13.0",
    "webpack-dev-middleware":          "1.6.1",
    "webpack-hot-middleware":          "2.10.0",

    "babel-loader":                    "6.2.4",
    "style-loader":                    "0.13.1",
    "css-loader":                      "0.23.1",
    "postcss-loader":                  "0.8.2",
    "json-loader":                     "0.5.4",

    "extract-text-webpack-plugin":     "1.0.1",
    "html-webpack-plugin":             "2.15.0",
    "stats-webpack-plugin":            "0.3.1",

    "autoprefixer":                    "6.3.6",

    "eslint":                          "2.8.0",
    "eslint-config-airbnb":            "7.0.0",
    "eslint-plugin-babel":             "3.2.0",
    "eslint-plugin-react":             "4.3.0",
    "babel-eslint":                    "6.0.2",

    "mocha":                           "2.4.5",
    "chai":                            "3.5.0",
    "enzyme":                          "2.2.0",
    "jsdom":                           "8.4.0",
    "sinon":                           "1.17.3",
    "mocha-jsdom":                     "1.1.0",
    "sinon-chai":                      "2.8.0",
    "chai-enzyme":                     "0.4.2",
    "chai-as-promised":                "5.3.0",

    "istanbul":                        "1.0.0-alpha.2",

    "rimraf":                          "2.5.2",
    "lodash.values":                   "4.1.0"
  }
}
